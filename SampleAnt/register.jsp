<html:html>
	<html:form action = "/register">
		<table border = 0 align = center>
			<tr>
				<th>UserName
				<td><html:text property = "username"/>
			</tr>
			<tr>
				<th>PassWord
				<td><html:password property = "password"/>
			</tr>
			<tr>
				<td colspan = 2 align = center>
					<html:submit value = "Register"/>
			</tr>
		</table>
	</html:form>
</html:html>